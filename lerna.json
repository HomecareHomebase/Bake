{
  "version": "independent",
  "npmClient": "npm",
  "command": {
    "publish": {
      "conventionalCommits": "true",
      "ignoreChanges": [
        "ignored-file",
        "*.md",
        "*,yaml"
      ],
      "message": "chore(release): publish"
    },
    "bootstrap": {
      "ignore": [
        "@azbake/arm-helper",
        "@azbake/ingredient-arm",
        "@azbake/ingredient-traffic-manager",
        "@azbake/ingredient-webapp-container"
      ]
    }
  },
  "packages": [
    "packages/*"    
  ],
  "version": "0.1.15"
}