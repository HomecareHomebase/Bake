{
  "version": "independent",
  "npmClient": "npm",
  "command": {
    "publish": {
      "conventionalCommits": "true",
      "ignoreChanges": [
        "ignored-file",
        "*.md",
        "*,yaml"
      ],
      "message": "chore(release): publish"
    }
  },
  "packages": [
    "packages/core",
    "packages/ingredient-script",
    "packages/ingredient-template",
    "packages/ingredient-arm",
    "packages/ingredient-utils",
    "packages/ingredient-traffic-manager"
  ],
  "version": "0.1.15"
}