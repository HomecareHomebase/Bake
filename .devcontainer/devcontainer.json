{
    "name": "Azure Bake Developement",
    "dockerFile": "Dockerfile",
    "appPort": 3000,
    "extensions": [
        "dbaeumer.vscode-eslint",
        "ms-vscode.vscode-typescript-tslint-plugin"
    ],
    "postCreateCommand": "npm install --global azure-bake && npm run clean:build && npm run load:ingredients",
    "runArgs": ["--env","BAKE_AUTH_SUBSCRIPTION_ID=$ENV:BAKE_AUTH_SUBSCRIPTION_ID","--env","BAKE_AUTH_SERVICE_ID=$ENV:BAKE_AUTH_SERVICE_ID","--env","BAKE_AUTH_SERVICE_KEY=$ENV:BAKE_AUTH_SERVICE_KEY","--env","BAKE_AUTH_TENANT_ID=$ENV:BAKE_AUTH_TENANT_ID","--env","BAKE_ENV_NAME=$ENV:BAKE_ENV_NAME","--env","BAKE_ENV_CODE=$ENV:BAKE_ENV_CODE","--env","BAKE_ENV_REGIONS=$ENV:BAKE_ENV_REGIONS","--env","BAKE_VARIABLES=$ENV:BAKE_VARIABLES","--env","BAKE_LOG_LEVEL=$ENV:BAKE_LOG_LEVEL"]
}